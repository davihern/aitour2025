{
    "version": "0.2.0",
    "configurations": [        
        {
            "name": "YourApp Sample",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/aitour2025/bin/Debug/net9.0/aitour2025.dll",
            "cwd": "${workspaceFolder}",
            "launchSettingsFilePath": "${workspaceFolder}/aitour2025/Properties/launchSettings.json",
            "launchSettingsProfile": "http",
            "env": {
                "SemanticKernelModel:ApiKey": "my_value",
                "ANOTHER_VARIABLE": "another_value"
            }
        },
        {
            "name": "Debug aitour2025tests",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/aitour2025tests/bin/Debug/net9.0/aitour2025tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/aitour2025tests",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "moduleLoad": false
            },
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}